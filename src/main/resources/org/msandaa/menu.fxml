<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>

<fx:root type="MenuBar" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
  <menus>
    <Menu mnemonicParsing="false" text="File">
      <items>
        <MenuItem mnemonicParsing="false" onAction="#openGraph" text="Read Graph" />
            <MenuItem mnemonicParsing="false" onAction="#openTrajektory" text="Read Trajectorys" />
      </items>
    </Menu>
      <Menu mnemonicParsing="false" text="Edit">
        <items>
          <MenuItem mnemonicParsing="false" onAction="#drawAll" text="Draw all Trajectoryies" />
            <MenuItem mnemonicParsing="false" onAction="#drawSelected" text="Draw selected Trajectories" />
        </items>
      </Menu>
  </menus>
</fx:root>
