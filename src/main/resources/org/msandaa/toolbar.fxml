<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Rectangle?>

<fx:root maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="300.0" prefWidth="101.0" type="VBox" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="RED" height="20.0" stroke="BLACK" strokeType="INSIDE" width="20.0" />
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#ff8000" height="20.0" stroke="BLACK" strokeType="INSIDE" width="20.0" />
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="YELLOW" height="20.0" stroke="BLACK" strokeType="INSIDE" width="20.0" />
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="#80ff00" height="20.0" stroke="BLACK" strokeType="INSIDE" width="20.0" />
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="LIME" height="20.0" stroke="BLACK" strokeType="INSIDE" width="20.0" />
      <CheckBox fx:id="speedCheckBox" focusTraversable="false" mnemonicParsing="false" onAction="#speedCheckBoxClicked" text="Speed">
         <padding>
            <Insets bottom="5.0" top="5.0" />
         </padding></CheckBox>
      <Slider fx:id="sliderMovesIn" blockIncrement="1.0" focusTraversable="false" majorTickUnit="1.0" max="5.0" minorTickCount="0" showTickLabels="true" showTickMarks="true" snapToTicks="true">
         <padding>
            <Insets bottom="5.0" top="5.0" />
         </padding></Slider>
      <Slider fx:id="sliderMovesOut" blockIncrement="1.0" focusTraversable="false" majorTickUnit="1.0" max="5.0" minorTickCount="0" showTickLabels="true" showTickMarks="true" snapToTicks="true">
         <padding>
            <Insets bottom="5.0" top="5.0" />
         </padding>
      </Slider>
      <Button focusTraversable="false" mnemonicParsing="false" onAction="#drawWall" prefHeight="25.0" prefWidth="100.0" text="Draw" />
      <Button focusTraversable="false" mnemonicParsing="false" onAction="#deleteWall" prefHeight="25.0" prefWidth="100.0" text="Delete" />
   </children>
</fx:root>
